{
  "manifest_version": 3,
  "name": "Google Job Searcher",
  "version": "1.1",
  "description": "Zbiera wyniki Google i eksportuje do CSV",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*"],
      "js": ["content.js"]
    }
  ]
}